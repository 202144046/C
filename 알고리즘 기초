#include <stdio.h>

int main(void)
{

    
    int dan=2;
    int count=1;




    while (count <= 9)
    {
        printf("%d + %d = %d\n",dan,count,dan*count);
        count = count + 1;
    }


}

#include <stdio.h>

int main(void)

{
    int sum=0;
    int count=1;
    
    while(count <= 100)
    {
        
        sum = sum + count;
        count = count + 1;
        
    }
    
    printf("%d",sum);
    
    return 0;
}

#include <stdio.h>

int main(void)

{
    int sum=0;
    int count=1;
    
    
    do{
        
        sum = sum + count;
        count = count + 1;
    }    
    while(count <= 100);
    
    printf("%d",sum);
    
    return 0;
}



