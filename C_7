#C언어 수업 7주차

#include <stdio.h>
int main(void)
{
    int a;
    int b;
    while (1)
    {
        printf("두 정수를 입력하시오. ");
        scanf("%d %d", &a,&b);
        if (a == 0 && b == 0)
    {
       break;
    }
        printf("%d\n", a + b);
    }
    return 0;
    
}


#include <stdio.h>

void print_stats()
{
    for(int i=0; i<30; i++)
    printf("*");
}

int main()
{
    print_stats();
    printf("\nHello, World!\n");
    print_stats();
    printf("\n");
    return 0;
}


#include <stdio.h>
int max(int x, int y);

int main(void)
{
    int x, y, larger;
    printf("정수 2개를 입력하시오 : ");
    scanf("%d %d", &x,&y);
    larger = max(x,y);
    printf("더 큰 값은 %d입니다. \n", larger);
    return 0;
}
int max(int x, int y)
{
    return x>y ? x : y ;
}



